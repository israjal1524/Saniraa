{
  "manifest_version": 3,
  "name": "Saniraa",
  "description": "Saniraa Auth0 Login Extension",
  "version": "1.0",
  "permissions": ["storage", "identity"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png.png",
      "48": "icons/icon.png.png",
      "128": "icons/icon.png.png"
    }
  },
  "icons": {
    "16": "icons/icon.png.png",
    "48": "icons/icon.png.png",
    "128": "icons/icon.png.png"
  },
  "background": {
    "service_worker": "auth.js"
  },
  "host_permissions": ["<all_urls>"]
}

